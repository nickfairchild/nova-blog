{
    "name": "nickfairchild/nova-blog",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova",
        "blog"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Nickfairchild\\NovaBlog\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nickfairchild\\NovaBlog\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
